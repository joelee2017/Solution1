    <Window.Triggers>
        <EventTrigger RoutedEvent="FrameworkElement.Loaded">
            <BeginStoryboard Storyboard="{StaticResource Storyboard1}"/>
        </EventTrigger>
    </Window.Triggers>




    <Grid Background="AliceBlue">

        <Grid.Resources>
            <ResourceDictionary>
                <ResourceDictionary.MergedDictionaries>

                    <ResourceDictionary Source="/WpfApp1;component/MyThemesStyle2/Dictionary_ShinyBlue.xaml"/>
                    <ResourceDictionary Source="/WpfApp1;component/MyThemesStyle/Dictionary_GelButtonResourceDictionary.xaml"/>
                    <ResourceDictionary Source="/WpfApp1;component/MyThemesStyle/Dictionary_ButtonStyle.xaml"/>

                    <ResourceDictionary Source="/WpfApp1;component/MyThemesStyle/Dictionary_DataGridStyle.xaml"/>

                </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>

        </Grid.Resources>
        <Grid.DataContext>
            <StaticResource ResourceKey="productPhotoViewSource"/>
        </Grid.DataContext>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="10"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock   Grid.ColumnSpan="3" x:Name="textBlock" Height="70" FontSize="40" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Showcard Gothic"  Width="529">
            <TextBlock.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="LightBlue" Offset="0"/>
                    <GradientStop Color="Blue" Offset="1"/>
                </LinearGradientBrush>
            </TextBlock.Foreground><Run Text="ProductPhotos CRUD"/><InlineUIContainer>
                    <ed:RegularPolygon x:Name="regularPolygon" Fill="Green" Height="19.946" InnerRadius="0.47211" PointCount="5" Stretch="Fill" Stroke="Black" Width="21.17" RenderTransformOrigin="0.5,0.5">
                        <ed:RegularPolygon.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </ed:RegularPolygon.RenderTransform>
                    </ed:RegularPolygon>
                </InlineUIContainer></TextBlock>

        <StackPanel Grid.ColumnSpan="3" Orientation="Vertical" Grid.Row="1" Margin="-6,0,6,0">
            <StackPanel Orientation="Horizontal" Height="82" Margin="0,0,0.333,0" RenderTransformOrigin="0.5,0.5" >
                <StackPanel.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform AngleX="1.215"/>
                        <RotateTransform/>
                        <TranslateTransform X="0.997"/>
                    </TransformGroup>
                </StackPanel.RenderTransform>
                <StackPanel.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="Gray" Offset="0"/>
                        <GradientStop Color="White" Offset="1"/>
                    </LinearGradientBrush>
                </StackPanel.Background>
                <Button Content="&lt;" Width="40" Margin="4" Style="{DynamicResource MyCircleButton}" Height="50" Click="Button_Click" />
                <Button Background="Blue"  Content="&gt;" Width="40" Margin="4" Style="{DynamicResource MyCircleButton}" Height="50" Click="Button_Click_1" />

                <TextBox x:Name="textBox1" Text="" Width="103" VerticalAlignment="Center"/>
                <TextBlock Text="~" FontSize="22" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                <TextBox x:Name="textBox2" Text="" RenderTransformOrigin="0.5,0.5" Width="91" VerticalAlignment="Center"/>

                <Button Background="Orange"  Content="Search" Width="50" Margin="4" Style="{DynamicResource MyGelButton}" Height="50" Click="Button_Click_2"  />

                <Button Background="Orange"  Content="Add" Width="50" Margin="4" Style="{DynamicResource MyGelButton}" Height="50" Click="Button_Click_3"  />
                <Button Background="Orange"  Content="Remove" Width="50" Margin="4" Style="{DynamicResource MyGelButton}" Height="50" Click="Button_Click_5"/>
                <Button Background="Orange"  Content="Modify" Width="50" Margin="4" Style="{DynamicResource MyGelButton}" Height="50" Click="Button_Click_6" />
                <Button Background="Orange"  Content="Add..." Width="50" Margin="4" Style="{DynamicResource MyGelButton}" Height="50" />
                <Button Background="Orange"  Content="Modify..." Width="50" Margin="4" Style="{DynamicResource MyGelButton}" Height="50"  />
                <Button Background="Blue"  Content="Update Batch" Width="86" Margin="4,16" Style="{DynamicResource MyGelButton}" Height="50" Click="Button_Click_4" />

            </StackPanel>
        </StackPanel>
        <GridSplitter  Grid.Column="1" Grid.Row="2" HorizontalAlignment="Stretch">
            <GridSplitter.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="White" Offset="1"/>
                </LinearGradientBrush>
            </GridSplitter.Background>
        </GridSplitter>
        <Grid  x:Name="grid1" Margin="10" Background="LightCoral" DataContext="{StaticResource productPhotoViewSource}" Grid.Row="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Label Content="Large Photo:" Grid.Column="0" HorizontalAlignment="Left" Margin="3" Grid.Row="0" VerticalAlignment="Center"/>
            <Image x:Name="largePhotoImage" Grid.Column="1" HorizontalAlignment="Left" Margin="3" Grid.Row="0" Source="{Binding LargePhoto}" VerticalAlignment="Center"/>
            <Label Content="Large Photo File Name:" Grid.Column="0" HorizontalAlignment="Left" Margin="3" Grid.Row="1" VerticalAlignment="Center"/>
            <TextBox x:Name="largePhotoFileNameTextBox" Grid.Column="1" HorizontalAlignment="Left" Height="23" Margin="3" Grid.Row="1" Text="{Binding LargePhotoFileName, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" VerticalAlignment="Center" Width="120"/>
            <Label Content="Modified Date:" Grid.Column="0" HorizontalAlignment="Left" Margin="3" Grid.Row="2" VerticalAlignment="Center"/>
            <DatePicker x:Name="modifiedDateDatePicker" Grid.Column="1" HorizontalAlignment="Left" Margin="3" Grid.Row="2" SelectedDate="{Binding ModifiedDate, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" VerticalAlignment="Center"/>
            <Label Content="Product Photo ID:" Grid.Column="0" HorizontalAlignment="Left" Margin="3" Grid.Row="3" VerticalAlignment="Center"/>
            <TextBox x:Name="productPhotoIDTextBox" Grid.Column="1" HorizontalAlignment="Left" Height="23" Margin="3" Grid.Row="3" Text="{Binding ProductPhotoID, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" VerticalAlignment="Center" Width="120"/>
            <Label Content="Thumb Nail Photo:" Grid.Column="0" HorizontalAlignment="Left" Margin="3" Grid.Row="4" VerticalAlignment="Center"/>
            <Image x:Name="thumbNailPhotoImage" Grid.Column="1" HorizontalAlignment="Left" Margin="3" Grid.Row="4" Source="{Binding ThumbNailPhoto}" VerticalAlignment="Center"/>
            <Label Content="Thumbnail Photo File Name:" Grid.Column="0" HorizontalAlignment="Left" Margin="3" Grid.Row="5" VerticalAlignment="Center"/>
            <TextBox x:Name="thumbnailPhotoFileNameTextBox" Grid.Column="1" HorizontalAlignment="Left" Height="23" Margin="3" Grid.Row="5" Text="{Binding ThumbnailPhotoFileName, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" VerticalAlignment="Center" Width="120"/>
        </Grid>
        <DataGrid x:Name="productPhotoDataGrid" AutoGenerateColumns="False" Grid.Column="2" EnableRowVirtualization="True" ItemsSource="{Binding}" Margin="25,8,19,7" Grid.Row="2" RowDetailsVisibilityMode="VisibleWhenSelected">
            <DataGrid.Columns>
                <DataGridTemplateColumn x:Name="largePhotoColumn" Header="Large Photo" Width="SizeToHeader">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Image Source="{Binding LargePhoto}"/>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <DataGridTextColumn x:Name="largePhotoFileNameColumn" Binding="{Binding LargePhotoFileName}" Header="Large Photo File Name" Width="SizeToHeader"/>
                <DataGridTemplateColumn x:Name="modifiedDateColumn" Header="Modified Date" Width="SizeToHeader">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <DatePicker SelectedDate="{Binding ModifiedDate, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}"/>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <DataGridTextColumn x:Name="productPhotoIDColumn" Binding="{Binding ProductPhotoID}" Header="Product Photo ID" Width="SizeToHeader"/>
                <DataGridTemplateColumn x:Name="thumbNailPhotoColumn" Header="Thumb Nail Photo" Width="SizeToHeader">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Image Source="{Binding ThumbNailPhoto}"/>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <DataGridTextColumn x:Name="thumbnailPhotoFileNameColumn" Binding="{Binding ThumbnailPhotoFileName}" Header="Thumbnail Photo File Name" Width="SizeToHeader"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>


</Window>
